  },
  "homepage": "https://github.com/qmOnArq/jsWebHacks#readme",
  "devDependencies": {
    "@types/file-saver": "^2.0.1",
    "@types/jquery": "^3.5.14",
    "@types/lodash": "^4.14.182",
    "clean-webpack-plugin": "^3.0.0",
    "jszip": "^3.7.0",
    "lodash": "^4.17.21",
    "prettier": "^2.7.1",
    "svelte": "^3.29.0",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^4.4.2",
    "terser-webpack-plugin": "^4.2.2",
    "ts-loader": "^8.4.0",
    "typescript": "^4.7.4",
